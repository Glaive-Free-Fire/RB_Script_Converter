import re

def process_data(input_str):
    """
    Обрабатывает входные данные и преобразует их в требуемый формат.
    
    Args:
        input_str (str): Входная строка с данными.
    
    Returns:
        str: Отформатированная строка в требуемом виде.
    """
    # Отфильтровываем пустые строки
    lines = [line.strip() for line in input_str.split('\n') if line.strip()]
    if not lines:
        return "Неверный формат входных данных"

    # Первая непустая строка должна содержать номер и заголовок
    first_line = lines[0]
    try:
        number, rest = first_line.split('=>', 1)
    except ValueError:
        return "Неверный формат первой строки, ожидается наличие '=>'"
    number = number.strip()
    title = rest.strip()
    
    # Используем регулярное выражение для извлечения двух списков
    m = re.search(r'\[\[\s*(.*?)\s*\],\s*\[\s*(.*)', input_str, re.DOTALL)
    if not m:
        return "Неверный формат списков"
    list1_str = m.group(1)
    list2_str = m.group(2).strip()
    # Если в конце второго списка есть завершающая скобка, удаляем её
    if list2_str.endswith(']'):
        list2_str = list2_str[:-1].strip()
    
    # Извлекаем строки из первого списка (описание)
    list1 = re.findall(r'"(.*?)"', list1_str)
    description = ''.join(list1)
    
    # Извлекаем строки из второго списка (экипировка и навыки)
    list2 = re.findall(r'"(.*?)"', list2_str)
    
    # Формируем выходную строку
    output = f"{number} {title}\n{description}\n\n"
    for item in list2:
        output += item + '\n'
    
    return output

# Пример использования с данными 354
input_data = """
354 => # キメラ
      [[
        "最先端の生命工学により造り出された人造モンスター",
        "極めて優れた肉体を持ち、圧倒的な戦闘能力を誇る",
        "炎や氷の他、電撃をブレスとして放射することが可能",
        "また肉体を自己再生する事もでき、生命力が非常に高い",
      ],
      [
        "装備防具：胸当て　甲羅　薄着　帽子　兜　軽盾　盾",
        "スキル：獣技　造技　ブレス",
        "アビリティ：なし",
"""

print(process_data(input_data))
